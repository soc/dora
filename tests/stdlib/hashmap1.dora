fun main() {
    let map = HashMap[Int, String]();
    map.insert(1, "a");
    map.insert(10000, "b");
    map.insert(7, "c");

    check_iterator1(map);

    assert(map.get(1).unwrap() == "a");
    assert(map.get(10000).unwrap() == "b");
    assert(map.get(7).unwrap() == "c");
    assert(map.get(2).isNone());
    assert(map.length == 3);
    assert(map.isEmpty().not());

    assert(map.contains(1));
    assert(map.contains(10000));
    assert(map.contains(7));
    assert(!map.contains(2));

    map.remove(10000);

    assert(map.get(1).unwrap() == "a");
    assert(map.get(10000).isNone());
    assert(map.get(7).unwrap() == "c");
    assert(map.get(2).isNone());
    assert(map.length == 2);
    assert(map.isEmpty().not());

    assert(map.contains(1));
    assert(!map.contains(10000));
    assert(map.contains(7));

    check_iterator2(map);
}

fun check_iterator1(map: HashMap[Int, String]) {
    let it = map.makeIterator();
    let set = BitSet(3);

    while it.hasNext() {
        let key_and_value = it.next();
        let key = key_and_value.0;
        let value = key_and_value.1;

        if key == 1 {
            assert(value == "a");
            assert(!set.contains(0));
            set.insert(0);
        } else if key == 10000 {
            assert(value == "b");
            assert(!set.contains(1));
            set.insert(1);
        } else if key == 7 {
            assert(value == "c");
            assert(!set.contains(2));
            set.insert(2);
        } else {
            unreachable();
        }
    }

    assert(set.contains(0));
    assert(set.contains(1));
    assert(set.contains(2));
}

fun check_iterator2(map: HashMap[Int, String]) {
    let it = map.makeIterator();
    let set = BitSet(3);

    while it.hasNext() {
        let key_and_value = it.next();
        let key = key_and_value.0;
        let value = key_and_value.1;

        if key == 1 {
            assert(value == "a");
            assert(!set.contains(0));
            set.insert(0);
        } else if key == 7 {
            assert(value == "c");
            assert(!set.contains(1));
            set.insert(1);
        } else {
            unreachable();
        }
    }

    assert(set.contains(0));
    assert(set.contains(1));
}