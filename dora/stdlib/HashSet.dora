class HashSet[K: Hash + Equals] {
    let map: HashMap[K, ()] = HashMap[K, ()]();

    fun insert(key: K) {
        self.map.insert(key, ());
    }

    fun contains(key: K) -> Bool {
        self.map.contains(key)
    }

    fun remove(key: K) {
        self.map.remove(key);
    }

    fun length -> Int {
        self.map.length
    }

    fun isEmpty -> Bool {
        self.map.isEmpty
    }

    fun capacity() -> Int {
        self.map.capacity()
    }
}